{
  "name": "Playforce Query",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.8",
    "codemirror": "^6.0.2",
    "node-fetch": "^3.3.2"
  },
  "build": {
    "appId": "...",
    "productName": "Playforce Query",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico"
    },
    "mac": null,
    "linux": null,
    "files": [
      "**/*"
    ],
    "asar": true
  }
}
